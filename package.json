{
  "name": "@valhalladev/movier",
  "version": "4.0.32",
  "description": "Javascript package for search and get details about IMDB movies and series",
  "license": "MIT",
  "author": "Ragnar Lothbrok <ragnarlothbrokjr@proton.me> and Zoha banam <zoha.banam@gmail.com> (https://www.linkedin.com/in/zoha-banam)",
  "homepage": "https://github.com/Valhalla-Development/movier",
  "repository": {
    "type": "git",
    "url": "https://github.com/Valhalla-Development/movier.git"
  },
  "bugs": {
    "url": "https://github.com/Valhalla-Development/movier/issues"
  },
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "keywords": [
    "imdb",
    "movie",
    "series",
    "tv shows"
  ],
  "scripts": {
    "test": "jest --config jest.config.ts --detectOpenHandles",
    "prepublish": "tsc",
    "lint": "tsc --noEmit -p tsconfig.json && biome check --files-ignore-unknown=true --diagnostic-level=error --no-errors-on-unmatched src",
    "lint:fix": "biome check --write --no-errors-on-unmatched --files-ignore-unknown=true src",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "change-case": "^5.4.4",
    "cheerio": "^1.1.2",
    "dayjs": "^1.11.19",
    "graphql": "^16.12.0",
    "graphql-request": "^7.3.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@tsconfig/node16": "^16.1.8",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.1",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "ultracite": "^6.4.0"
  }
}
